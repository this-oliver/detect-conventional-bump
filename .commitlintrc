{
  "extends": [
    "@commitlint/config-conventional"
  ],
  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "major",
        "breaking",
        "minor",
        "feat",
        "ft",
        "patch",
        "bug",
        "fix",
        "chore",
        "docs"
      ]
    ]
  }
}